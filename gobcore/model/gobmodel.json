{
  "meetbouten": {
    "version": "0.1",
    "entity_id": "meetboutidentificatie",
    "attributes": {
      "meetboutidentificatie": {
        "type": "GOB.String",
        "description": "Unieke identificatie van de meetbout"
      },
      "nabij_nummeraanduiding": {
        "type": "GOB.Reference",
        "description": "Een adres in de nabijheid van de meetbout",
        "ref": "bag:nummeraanduidingen"
      },
      "locatie": {
        "type": "GOB.String",
        "description": "Beschrijving van de locatie van de meetbout, bijvoorbeeld 'in gemeenschappelijke muur'"
      },
      "status": {
        "type": "GOB.JSON",
        "description": "Status van de meetbout (1=actueel, 2=niet te meten, 3=vervallen)"
      },
      "vervaldatum": {
        "type": "GOB.Date",
        "description": "Datum waarop de meting heeft plaatsgevonden"
      },
      "merk": {
        "type": "GOB.JSON",
        "description": "Merk"
      },
      "x_coordinaat_muurvlak": {
        "type": "GOB.Decimal",
        "description": "X-coördinaat muurvlak"
      },
      "y_coordinaat_muurvlak": {
        "type": "GOB.Decimal",
        "description": "Y-coördinaat muurvlak"
      },
      "windrichting": {
        "type": "GOB.String",
        "description": "Windrichting"
      },
      "ligt_in_bouwblok": {
        "type": "GOB.Reference",
        "description": "Het bouwblok waarbinnen de meetbout ligt",
        "ref": "gebieden:bouwblokken"
      },
      "ligt_in_buurt": {
        "type": "GOB.Reference",
        "description": "De buurt waarbinnen de meetbout ligt",
        "ref": "gebieden:buurten"
      },
      "ligt_in_stadsdeel": {
        "type": "GOB.Reference",
        "description": "Het stadsdeel waarbinnen de meetbout ligt",
        "ref": "gebieden:stadsdelen"
      },
      "geometrie": {
        "type": "GOB.Geo.Point",
        "srid": "RD",
        "description": "Geometrische ligging van de meetbout"
      },
      "publiceerbaar": {
        "type": "GOB.Boolean",
        "description": "Publiceerbaar ja of nee"
      }
    },
    "api": {
      "filters": [
        {
          "field": "_source",
          "op": "==",
          "value": "Grondslag"
        },
        {
          "field": "publiceerbaar",
          "op": "==",
          "value": true
        }
      ]
    }
  },
  "metingen": {
    "version": "0.1",
    "entity_id": "metingidentificatie",
    "attributes": {
      "metingidentificatie": {
        "type": "GOB.String",
        "description": "Unieke identificatie van de meting"
      },
      "hoort_bij_meetbout": {
        "type": "GOB.Reference",
        "description": "De meetbout waarop de meting is gedaan",
        "ref": "meetbouten:meetbouten"
      },
      "datum": {
        "type": "GOB.Date",
        "description": "Datum waarop de meting heeft plaatsgevonden"
      },
      "type_meting": {
        "type": "GOB.Character",
        "description": "Type meting (N=nulmeting, H=herhalingsmeting, T=tussentijdse meting, S=schatting)"
      },
      "wijze_van_inwinnen": {
        "type": "GOB.JSON",
        "description": "Wijze waarop de meting is ingewonnen"
      },
      "hoogte_tov_nap": {
        "type": "GOB.Decimal",
        "description": "Gemeten hoogte van de meetbout tov NAP"
      },
      "zakking": {
        "type": "GOB.Decimal",
        "description": "Zakking van de meting t.o.v. vorige meting (mm)"
      },
      "refereert_aan": {
        "type": "GOB.ManyReference",
        "description": "De referentiepunten waar de metingen aan worden opgehangen",
        "ref": "meetbouten:referentiepunten"
      },
      "zakkingssnelheid": {
        "type": "GOB.Decimal",
        "description": "Zakkingssnelheid van de meetbout (mm/​jaar) sinds de meting"
      },
      "zakking_cumulatief": {
        "type": "GOB.Decimal",
        "description": "Cumulatieve zakking sinds de plaatsing van de meetbout"
      },
      "is_gemeten_door": {
        "type": "GOB.String",
        "description": "ID Van de onderneming die de meting heeft uitgevoerd"
      },
      "hoeveelste_meting": {
        "type": "GOB.Integer",
        "description": "Hoeveelste meting van de meetbout"
      },
      "aantal_dagen": {
        "type": "GOB.Integer",
        "description": "Het aantal dagen sinds de vorige meting"
      }
    },
    "api": {
      "filters": [
        {
          "field": "_source",
          "op": "==",
          "value": "Grondslag"
        }
      ]
    }
  },
